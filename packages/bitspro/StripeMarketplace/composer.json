{
    "name": "bitspro/stripemarketplace",
    "description": "A simple package to manage strip connect bindings.",
    "license": "license",
    "authors": [
        {
            "name": "Rizwan Ali",
            "email": "riz@bitspro.com",
            "homepage": "https://www.bitspro.com"
        }
    ],
    "homepage": "https://github.com/bitspro/stripemarketplace",
    "keywords": [
        "Laravel",
        "StripeMarketplace"
    ],
    "require": {
        "illuminate/support": "~5",
        "stripe/stripe-php": "^7.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "bitspro\\StripeMarketplace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bitspro\\StripeMarketplace\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bitspro\\StripeMarketplace\\StripeMarketplaceServiceProvider"
            ],
            "aliases": {
                "StripeMarketplace": "bitspro\\StripeMarketplace\\Facades\\StripeMarketplace"
            }
        }
    }
}
